
%title 'Welcome11111';

%if (defined( $images ) && scalar(@$images) > 0 ){
  <ul id="myGallery" class="thumbnails">
    %foreach my $image ( @$images ){
        <li class="span2">
          <a href="#" class="thumbnail">
            <img src="<%= $image %>" alt="">
          </a>          
        </li>
    %}
  </ul>
%}
